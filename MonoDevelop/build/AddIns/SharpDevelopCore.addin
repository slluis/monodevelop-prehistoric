				             class = "MonoDevelop.Commands.BuildCurrentProject"/>
				<ToolbarItem id = "Run"
				             icon = "Icons.16x16.RunProgramIcon"
				             tooltip = "${res:XML.MainMenu.RunMenu.Run.Description}"
				             class = "MonoDevelop.Commands.RunCommand"/>
			</Conditional>
		</ToolbarItem>
	</Extension>

